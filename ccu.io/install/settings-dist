# Allgemeine Variablen
export CCUIO_PATH="/opt/ccu.io"              # Hier den Pfad angeben in dem sich ccu.io befindet
export CCUIO_CMD="/etc/init.d/ccu.io.sh"     # Hier den Aufruf von ccu.io angeben
export CCUIO_USER=pi                         # Hier den User angeben unter dem ccu.io laufen soll

# Addons und ccu.io
export DASHUI=true                           # DashUI installieren = 1
export CHARTS=true                           # CCU-IO-Highcharts installieren = 1
export YAHUI=true                            # yahui installieren = 1
export EVENTLIST=true                        # CCU-IO Eventlist installieren = 1
